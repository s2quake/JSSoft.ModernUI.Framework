<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:nmfc="clr-namespace:Ntreev.ModernUI.Framework.Controls">

    <ControlTemplate x:Key="NumericTextBox_Template" TargetType="{x:Type nmfc:NumericTextBox}">
        <TextBox x:Name="PART_TextBox" />
    </ControlTemplate>

    <Style TargetType="{x:Type nmfc:NumericTextBox}">
        <Setter Property="Template" Value="{StaticResource NumericTextBox_Template}" />
    </Style>
    
</ResourceDictionary>